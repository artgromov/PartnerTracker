{
    "version": 1,
    "root": {
        "handlers": [
            "console"
        ],
        "level": "DEBUG"
    },
    "loggers": {
        "partner_tracker": {
            "level": "DEBUG"
        },
        "partner_tracker.driver": {
            "level": "ERROR"
        },
        "partner_tracker.partner": {
            "level": "ERROR"
        },
        "partner_tracker.prints": {
            "level": "DEBUG"
        },
        "partner_tracker.providers": {
            "level": "ERROR"
        },
        "partner_tracker.searchers": {
            "level": "ERROR"
        },
        "cli.blocks": {
            "level": "DEBUG"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "formatter": "default",
            "stream": "ext://sys.stdout",
            "level": null
        }
    },
        "formatters": {
        "default": {
            "format": "%(name)s: %(levelname)s: %(message)s"
        }
    }
}
